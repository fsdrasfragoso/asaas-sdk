{
    "name": "fsdrasfragoso/asaas-sdk",
    "description": "SDK para a API Asaas. Este SDK permite a integração com funcionalidades como pagamentos, subscrições, notificações, entre outras.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Asaas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asaas\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "authors": [
        {
            "name": "Esdras Fragoso Da Silva Neto",
            "email": "esdrasfragoso@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit --configuration phpunit.xml"
    }
}
